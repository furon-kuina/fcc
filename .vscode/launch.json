  "version": "0.2.0",
  "configurations": [
    
    {
        "name": "launch_cpp",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/fcc",
        "args": ["main(){x=3;y=5;z=&y+8;return *z;}"],
        "stopAtEntry": false,
        "cwd": "${fileDirname}",
        "environment": [],
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
            {
                "description": "gdb の再フォーマットを有効にする",
                "text": "-enable-pretty-printing",
                "ignoreFailures": true
            }
        ]
    }
]
}